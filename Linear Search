#include <stdio.h>

int main() {
    int arr[] = {2, 3, 4, 10, 40};
    int n = sizeof(arr) / sizeof(arr[0]); 
    int target = 10;
    int found = 0; 
    int index = -1; 
    for (int i = 0; i < n; i++) {
        if (arr[i] == target) 
            found = 1;
            index = i;
            break; 
        }
    }
    if (found)
        printf("Element is present at index %d\n", index);
    else
        printf("Element is not present in array\n");

    return 0;
}
